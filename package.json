{
  "name": "figma",
  "version": "1.0.0",
  "main": "src/renderer/index.tsx",
  "author": "elase",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development PORT=9000 WITH_MAIN=ture TS_NODE_TRANSPILE_ONLY=true webpack serve --config config/webpack.config.renderer.js",
    "start:main": "cross-env NODE_ENV=development PORT=9000 electron -r ts-node/register/transpile-only src/main/index.ts",
    "start:renderer": "cross-env NODE_ENV=development PORT=9000 WITH_MAIN=false TS_NODE_TRANSPILE_ONLY=true webpack serve --config config/webpack.config.renderer.js",
    "run": "electron dist/main/main.js",
    "build": "yarn run build:main && yarn run build:renderer",
    "build:main": "cross-env NODE_ENV=production webpack --config config/webpack.config.main.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config config/webpack.config.renderer.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/builder-webpack5": "^6.3.12",
    "@storybook/manager-webpack5": "^6.3.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.12",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "electron": "^16.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "node-sass": "^6.0.1",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  },
  "dependencies": {
    "canvaskit-wasm": "^0.31.0",
    "mobx": "^6.3.7",
    "mobx-react": "^7.2.1",
    "path-browserify": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "^7.4.0"
  }
}
